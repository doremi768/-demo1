/* pages/profile/prodile.wxss */
page{
  background-color: #f1f1f1;
}
.profile-header{
  background-image: url(data:image/png;base64,);
  background-size: cover;
  height: 480rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0,0,0.3);
}

.avatar-url{
  width: 200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border: 6rpx solid #fff;
  border-radius: 50%;
}
.nickname{
  margin-top: 10rpx;
}

.nav{
  overflow: hidden;
  margin: 50rpx 30rpx;
  text-align: center;
  border-radius: 35rpx;
  background-color: white;
}
.nav-item{
  line-height: 150rpx;
  border: 1px solid #E8E8E8;
}
.content{
  position: relative;
  text-align: left;
  padding-left: 100rpx;
  color: #696969;
}
.icon-bofangzhong, .icon-soucang-copy-copy-copy, .icon-qrcode{
  position: absolute;
  left: 40rpx;
  color: #d43c43;
  font-size: 40rpx;
  font-weight: 600;

}
.icon-arr-right{
 position: absolute;
 right: 60rpx;
}